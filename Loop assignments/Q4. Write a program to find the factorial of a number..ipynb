   "source": [
    "#Q4. Write a program to find the factorial of a number.\n",
    "\n",
    "\n",
    "num = int(input(\"Enter the number:\"))\n",
    "fact = 1\n",
    "i = num\n",
    "while i>1:\n",
    "    fact = fact*i\n",
    "    i = i-1\n",
    "print(\"factorial of\",num,\"is\", fact)"
   ]
